{
    "manifest_version": 2,
    "name": "FocusGuard",
    "version": "3.2",
    "description": "Block distracting websites",
    "permissions": [
      "storage",
      "tabs",
      "webRequest",
      "webRequestBlocking",
      "*://*/*"
    ],
    "background": {
      "scripts": ["background.js"], 
      "persistent": true
    },
    "browser_action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "browser_specific_settings": {
      "gecko": {
        "id": "FocusGuard@Auraaid.netlify.com",
        "strict_min_version": "91.0"
      }
    }
  }
  
